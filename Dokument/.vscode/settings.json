{
    "ltex.language": "de-DE",
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "--output-directory=%OUTDIR%",
                "%DOC%"
            ]
        }
    ],
    "cSpell.language": "en,de-DE",
    "cSpell.words": [
        "Containerisierung"
    ]

}